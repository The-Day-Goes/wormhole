{
  "compilerOptions": {
    "target": "ES2016",
    "module": "esnext",
    "lib": ["dom", "webworker"],
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "rootDir": "./src",
    "moduleResolution": "node"
  }
}
